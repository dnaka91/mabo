{
  "name": "stef",
  "displayName": "STEF",
  "description": "",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/dnaka91/stef.git"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=8",
    "vscode": "^1.81.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "stef",
        "aliases": [
          "STEF",
          "stef"
        ],
        "extensions": [
          ".stef"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "stef",
        "scopeName": "source.stef",
        "path": "./syntaxes/stef.tmLanguage.json"
      }
    ]
  },
  "vsce": {
    "dependencies": false
  },
  "scripts": {
    "vscode:prepublish": "js-yaml syntaxes/stef.tmLanguage.yaml > syntaxes/stef.tmLanguage.json",
    "package": "vsce package"
  },
  "devDependencies": {
    "@vscode/vsce": "^2.21.0",
    "js-yaml": "^4.1.0"
  }
}
